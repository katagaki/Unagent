    "SetUp.Step1" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "variations" : {
            "device" : {
              "mac" : {
                "stringUnit" : {
                  "state" : "translated",
                  "value" : "1. Open the Safari app.\n2. From the menu bar, select 'Safari' > 'Preferences...'.\n3. Select the Extensions tab.\n4. Select 'Unagent'.\n5. Turn Unagent on by checking it."
                }
              },
              "other" : {
                "stringUnit" : {
                  "state" : "translated",
                  "value" : "1. Open the Settings app.\n2. Scroll down and tap 'Safari.'\n3. Tap 'Extensions'.\n4. Tap 'Unagent'.\n5. Toggle Unagent on for the profiles you wish to use Unagent in."
                }
              }
            }
          }
        },
        "ja" : {
          "variations" : {
            "device" : {
              "mac" : {
                "stringUnit" : {
                  "state" : "translated",
                  "value" : "① 「Safari」アプリを開きます。\n② メニューバーから「Safari」＞「設定…」の順に選択します。\n③ 拡張機能タブを選択します。\n④ 「Unagent」を選択します。\n⑤ Unagentをチェックして有効化します。"
                }
              },
              "other" : {
                "stringUnit" : {
                  "state" : "translated",
                  "value" : "① 「設定」アプリを開きます。\n② 下にスクロールし、「Safari」をタッチします。\n③ 「拡張機能」をタッチします。\n④ 「Unagent」をタッチします。\n⑤ Unagentを有効化するプロフィールをオンにします。"
                }
              }
            }
          }
        },
        "pt-BR" : {
          "variations" : {
            "device" : {
              "mac" : {
                "stringUnit" : {
                  "state" : "translated",
                  "value" : "1. Abra o Safari.\r\n2. Na barra de menus, selecione 'Safari' > ‘Preferências...'.\r\n3. Selecione a aba 'Extensões'.\r\n4. Selecione ‘Unagent'.\r\n5. Ative a opção 'Unagent'."
                }
              },
              "other" : {
                "stringUnit" : {
                  "state" : "translated",
                  "value" : "1. Abra os Ajustes.\r\n2. Role para baixo e clique em 'Safari'.\r\n3. Clique em 'Extensões'.\r\n4. Clique em 'Unagent'.\r\n5. Ative o Unagent para os perfis onde você deseja usar o Unagent."
                }
              }
            }
          }
        }
      }
    },
    "SetUp.Step1.iOS18+" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "1. Open the Settings app.\n2. Scroll down and tap 'Apps'.\n3. Find and tap 'Safari'.\n4. Tap 'Extensions'.\n5. Tap 'Unagent'.\n6. Toggle Unagent on for the profiles you wish to use Unagent in."
          }
        },
        "ja" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "① 「設定」アプリを開きます。\n② 下にスクロールし、「アプリ」をタッチします。\n③ 「Safari」を探してタッチします。\n④ 「拡張機能」をタッチします。\n⑤ 「Unagent」をタッチします。\n⑥ Unagentを有効化するプロフィールをオンにします。"
          }
        },
        "pt-BR" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "1. Abra os Ajustes.\r\n2. Role para baixo e clique em 'Apps'.\n3. Role para baixo e clique em 'Safari'.\r\n4. Clique em 'Extensões'.\r\n5. Clique em 'Unagent'.\r\n6. Ative o Unagent para os perfis onde você deseja usar o Unagent."
          }
        }
      }
