                "stringUnit" : {
                  "state" : "translated",
                  "value" : "1. Open the Safari app.\n2. Tap the 'Aa' icon to the left of the address bar.\n3. Tap 'Unagent'.\n4. Tap 'Always Allow...'.\n5. Tap 'Always Allow on Every Website'."
                }
